    "#Gives us the energy of the state\n",
    "def Energy(array):\n",
    "    Enn=0\n",
    "    for i in range(0,N):\n",
    "        for j in range(0,N): \n",
    "            for k in range(0,N):\n",
    "                Enn = Enn - spin(i,j,k)\n",
    "    return (1/2)*Enn\n",
    "    "
   ]
  },
