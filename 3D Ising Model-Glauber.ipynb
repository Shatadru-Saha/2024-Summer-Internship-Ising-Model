   "source": [
    "for i in range(0,N):\n",
    "    for j in range(0,N):\n",
    "        for k in range(0,N):\n",
    "            if x[i,j,k] < 0.5:\n",
    "                x[i,j,k]=1\n",
    "            else:\n",
    "                x[i,j,k]=-1 #Generates a T= inf state"
   ]
  },
