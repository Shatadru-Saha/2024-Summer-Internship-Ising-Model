    "#This function implements the metropolis algorithm and calculates the needed quantities\n",
    "def calculate(x,T):\n",
    "    H = Energy(x)\n",
    "    m = mag(x)\n",
    "    steps=0\n",
    "    E_plot=[H]\n",
    "    magnetisations=[m]\n",
    "    \n",
    "    for i in range(4*10**6):\n",
    "        p = np.random.randint(0,N)\n",
    "        q = np.random.randint(0,N)\n",
    "        r = np.random.randint(0,N)\n",
    "        spin_i=x[p,q,r]\n",
    "        steps = steps +1\n",
    "        dE = 2*spin(p,q,r)\n",
    "        if dE <=0:\n",
    "            x[p,q,r]=-x[p,q,r]\n",
    "            E_new = H + dE\n",
    "            m_new= m +(-2)*spin_i\n",
    "        else:\n",
    "            temp=np.random.rand()\n",
    "            prob = np.exp(-(dE)/T)\n",
    "            if temp < prob:\n",
    "                x[p,q,r]=-x[p,q,r]\n",
    "                E_new = H + dE\n",
    "                m_new= m +(-2)*spin_i\n",
    "            else:\n",
    "                E_new = H\n",
    "                m_new= m\n",
    "        H = E_new\n",
    "        m = m_new\n",
    "        E_plot.append(E_new)\n",
    "        magnetisations.append(m_new)\n",
    "        \n",
    "    E_calc = np.array(E_plot)\n",
    "    M_calc = np.array(magnetisations)\n",
    "    \n",
    "    E_avg= E_calc.mean()\n",
    "    C= E_calc.std()/(T**2)\n",
    "    M_avg= M_calc.mean()\n",
    "    Chi = M_calc.std()/T\n",
    "    \n",
    "    M_avg = M_avg/ (N**3)\n",
    "    \n",
    "    return [E_avg , M_avg, C, Chi]\n"
   ]
  },
